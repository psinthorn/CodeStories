<div style="padding: 10px">

<div class="row">

    {{> _msg}}

    <div class="row">
        
        <div class="col s12 m8 l8 ml-3">
                        <table class="highlight">
                            <thead>
                            <tr>
                                <th>From</th>
                                <th>To</th>
                                <th>Sedan</th>
                                <th>SUV</th>
                                <th>Mini Bus</th>
                            </tr>
                            </thead>
                          
                            <tbody>

                            {{#each transfers }}
                            <tr>
                                <a href="/transfers-show/{{ id }}">
                                <td>{{ from }}</td>
                                <td>{{ to }}</td>
                                <td>{{ sedan.sale }}</td>
                                <td>{{ suv.sale }}</td>
                                <td>{{ minibus.sale }}</td>
                                 </a>
                            </tr>
                            {{/each}}
                            </tbody>
                    </table>

                <br>
        </div>
    </div>
</div>

</div>